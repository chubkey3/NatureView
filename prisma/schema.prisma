generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Image {
  filename     String   @id @unique @db.VarChar(255)
  lastModified DateTime @db.Date
  url          String   @unique @db.VarChar(333)
  category     String?  @default("nature")
  description  String?  @db.Text
  width        Int?      @default(100) @db.Int
  height       Int?      @default(100) @db.Int
}
