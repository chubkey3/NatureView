generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Image {
  filename     String  @db.VarChar(255) @unique @id
  lastModified DateTime @db.Date
  url          String  @db.VarChar(333) @unique
  category     String? @default("nature")
  description  String? @db.Text
}
